{
    "name": "ivory/google-map-bundle",
    "type": "symfony-bundle",
    "description": "Provides a google map integration for Symfony",
    "keywords": [ "google", "map" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Breu",
            "email": "samuel.breu@bresam.ch",
            "role": "Developer/Maintainer"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com",
            "role": "Creator/Inactive"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "symfony/framework-bundle": "^4.4 || ^5.4 || ^6.2",
        "ivory/google-map": "dev-feature/add-supportfor-symfony-6"
    },
    "require-dev": {
        "ivory/serializer-bundle": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.19 || 3.14",
        "php-http/cache-plugin": "^1.7.5",
        "php-http/guzzle6-adapter": "^1.0 || ^2.0",
        "phpunit/phpunit": "^8.5 || ^9.6",
        "phpunit/phpunit-selenium": "^8.0 || ^9.0",
        "symfony/cache": "^4.4 || ^5.4 || ^6.2",
        "symfony/form": "^4.4 || ^5.4 || ^6.2",
        "symfony/phpunit-bridge": "^4.4 || ^5.4 || ^6.2",
        "symfony/templating": "^4.4 || ^5.4 || ^6.2",
        "symfony/yaml": "^4.4 || ^5.4 || ^6.2",
        "twig/twig": "^2.15 || ^3.5"
    },
    "autoload": {
        "psr-4": { "Ivory\\GoogleMapBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Ivory\\Tests\\GoogleMap\\": "vendor/ivory/google-map/tests" }
    },
    "replace": {
        "egeloen/google-map-bundle": "^3.0.1"
    },
    "repositories": {
        "repo-ivory/google-map": {
            "type": "vcs",
            "url": "git@github.com:Chris53897/ivory-google-map.git"
        },
        "repo-ivory-serializer": {
            "type": "vcs",
            "url": "git@github.com:Chris53897/ivory-serializer.git"
        }
    }
}
